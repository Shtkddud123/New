#include <iostream>
#include "Sally.h"
#include <vector>
#include <algorithm>

using namespace std;

Sally::Sally() {
}

Sally::Sally(int a) {
  num = a; 
}

Sally Sally::operator+(Sally aso) { // a sallyo object 
  Sally brandNew;
  // new sally object num is this->num + aso.nu,
  brandNew.num =  this->num + aso.num;
  return(brandNew); // returns the brandnew sally object 
}

